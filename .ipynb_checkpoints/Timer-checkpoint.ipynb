   "source": [
    "from threading import Timer\n",
    "\n",
    "def timeout():\n",
    "    print(\"Game over\")\n",
    "\n",
    "# duration is in seconds\n",
    "t = Timer( 5, timeout)\n",
    "t.start()\n",
    "\n",
    "# wait for time completion\n",
    "t.join()"
   ]
